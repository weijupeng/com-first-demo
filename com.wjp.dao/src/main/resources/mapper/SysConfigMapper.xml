<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wjp.dao.mapper.SysConfigMapper">
    <resultMap id="BaseResultMap" type="com.wjp.dao.entity.SysConfig">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="config_name" property="configName" jdbcType="VARCHAR"/>
        <result column="config_value" property="configValue" jdbcType="VARCHAR"/>
        <result column="config_desc" property="configDesc" jdbcType="VARCHAR"/>
        <result column="is_delete" property="isDelete" jdbcType="BIT"/>
    </resultMap>
    <sql id="Base_Column_List">
    id, config_name, config_value, config_desc, is_delete, create_time, update_time
  </sql>
    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long">
        select
        <include refid="Base_Column_List"/>
        from t_sys_config
        where is_delete = 0 and id = #{id,jdbcType=BIGINT}
    </select>
</mapper>