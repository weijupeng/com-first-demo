            <if test="couponId != null">
                coupon_id,
            </if>
            <if test="couponValue != null">
                coupon_value,
            </if>
            <if test="supportSubjects != null">
                support_subjects,
            </if>
            <if test="highLimitCash != null">
                high_limit_cash,
            </if>
            <if test="lowerLimitCash != null">
                lower_limit_cash,
            </if>
            <if test="loanPeriods != null">
                loan_periods,
            </if>
            <if test="isDeleted != null">
                is_deleted,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
