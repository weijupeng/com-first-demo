      select * from t_coupon_cash where coupon_id =#{couponId} and is_deleted = 0
    </select>
    <select id="getByCouponIds" resultType="com.wjp.dao.dao.entity.CouponCash">
        select * from t_coupon_cash where is_deleted = 0 and coupon_id in
        <foreach collection="couponIds" item="item" separator="," open="(" close=")">
            #{item}
        </foreach>
    </select>
